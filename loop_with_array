let num = [523,23,465,77,64]

for( let i=0; i< num.length; i++){
   console.log(num[i])
}

//for each loop

num.forEach((element) => {
    console.log(element)
    console.log(element * 2)

}) 

// array.from

let name = "sahil"
let arr = Array.from(name)
console.log(arr)

// for of 

for(let i of num){
  console.log(i)
}